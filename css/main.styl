							font-size 1.2em
							.view-more
								float left
			.find-branches
				background-color transparent
				text-align left
				padding-left 37%
				a
					width 54%
					background azulClaro
					padding 10px 40px
					border-radius 4px
					bottom 55px
					position relative
		.newsletter
			.title
				width 70%
			form
				width 420px
		/*
		Business branches
		*/
		.container-fluid
			figure
				img
					margin-left -15%
					width 130%
		.all-content
			padding-bottom 20px
			padding-top 20px
			.find-branches
				width 50%
				margin 30px auto
		.branches-container
			.col
				width 46.5%
				display inline-block
				position relative
				margin-right 3%
				figure
					padding 0
					margin 0
					&.logo
						width 50%
						position absolute
						top 50%
						left 50%
						margin-left -25%
						margin-top -20%
					img
						width 100%
						height auto
		
		/*Festeja con nosotros*/
		.festeja-con-nosotros
			.title-with-shadow
				top initial
				bottom 15px
			.container-fluid figure img
				width 110%
				margin-left -5%
