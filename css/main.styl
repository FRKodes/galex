rojo = #E72A48
azul = #0C18C4
azulClaro = #01AEF0
azulMedio = #1866B0
amarillo = #FFC600
.container-fluid.brands
	overflow visible
.form-control
	border 1px solid azulClaro
	color azulClaro
	resize none

.styled-select select
	background transparent
	width 100%
	padding 0 0 0 10px
	font-size 16px
	line-height 1
	border 0
	color #999
	font-size .95em
	height 34px
	border-radius 0
	-webkit-appearance none

.styled-select
	background white url(./images/arrow.png) no-repeat right 2px
	border 1px solid azulClaro
	border-radius 4px
	height 34px
	overflow hidden
	width 100%
.sucursales
	.styled-select select
		background transparent
		border 0
		color white
		font-size .95em
		text-align center
	.styled-select
		background azulClaro url(./images/arrow.png) no-repeat right 2px
		border 1px solid azulClaro
	  
.btn.btn-primary
	background-color azulClaro
	border-color azulClaro
	width 100%
	
body
	background-color #F6F6F6
	color azulClaro
	font-family 'Yanone Kaffeesatz', sans-serif
	padding-top 70px
	.navbar-default
		background-color #0A18C3
		border-bottom 3px solid rojo
		height 70px
		overflow visible
		.navbar-header
			overflow visible
			.navbar-toggle
				border-color transparent
				margin-top 14px
				&:hover
				&:focus
					background-color transparent
				.icon-bar
					background-color white
					height 4px
					width 30px
			.navbar-brand
				background transparent url(./images/logo-galex-mobile.png) no-repeat left top
				color transparent
				height 70px
				margin 12px 0 0 10px
				position relative
				width 110px
				&:hover
				&:focus
				&:active
					color transparent
	.near-location
		display none
	.main-banner
		background transparent url(./images/fondo-banner-principal.jpg) no-repeat center top
		background-size cover
		border-bottom 5px solid rojo
		height 400px
		width 100%
		.item
			overflow hidden
			height 395px
			position relative
			img
				width 200%
				margin-left -25%
				position absolute
				bottom 0
		.slick-prev
		.slick-next
			position absolute
			top 45%
			font-size 5em
			color rojo
			background-color transparent
			border none
			button
				&:focus
				&:active
				&:hover
					outline none
		.slick-prev
			left 0
			z-index 10
		.slick-next
			right 0
	.brands
		border-bottom 5px solid rojo
		padding-bottom 20px
		.block-unity
			margin 20px 0 0
			padding 0
			position relative
			a
				img
					display block
					height auto
					width 100%
				&.logo-brand
					position absolute
					top 30%
					left 5%
					width 45%
	.promos-banner
		background transparent url(./images/fondo-experiencia.png) no-repeat center center
		background-size cover
		height auto
		overflow hidden
		.item
			padding-top 20px
			position relative
			.title
				text-align center
				img
					height auto
					width 90%
			.top
				display block
				height 400px
				margin-top -50px
				overflow hidden
				ul
					display block
					overflow hidden
					padding 0
					li
						display inline-block
						overflow hidden
						position absolute
						img
							height auto
							margin-left -30%
							width 160%
			.bottom
				color white
				margin-top -140px
				padding-bottom 20px
				position relative
				width 100%
				.slogan
					font-size 3em
					text-align center
					span
						text-shadow 4px 0 0 azul, -2px 0 0 azul, 0 2px 0 azul, 0 -2px 0 azul, 2px 2px azul, -2px -2px 0 azul, 2px -2px 0 azul, -2px 2px 0 azul
				p
					margin 0
					padding 0 5%
					text-align center
	.place
		background-color rojo
		overflow hidden
		.section-title
			border-bottom 4px solid azulClaro
			display block
			height auto
			margin 0 auto
			padding 20px 5% 10px 5%
			img
				width 100%
		.place-carousel
			.inner-section-title
			.place-map
				display none
			.item
				img
					height auto
					width 100%
				.title
					background rgba(azulClaro,0.8)
					color white
					font-size 1.3em
					height 50px
					line-height 50px
					margin-top -50px
					padding 0 5%
					position relative
				.map-info-container
					width 90%
					margin 0 auto
					padding-top 10px
					.info
						color white
						overflow hidden
						padding-top 10px
						.view-more
							color white
							float right
							margin 10px 0
							text-decoration underline
	.find-branches
		background azulClaro url(./images/pin.png) no-repeat left center
		border-radius 3px
		margin 10px auto
		padding 10px 0
		text-align center
		width 94%
		a
			color white
	.newsletter
		background transparent url(./images/fondo-newssletter-galex.jpg) no-repeat center top
		background-size cover
		padding 30px 0
		.title
			margin 20px auto
			width 90%
			img
				height auto
				width 100%
		form
			margin 0 auto 30px auto
			width 90%
	/*
	Business branches
	*/
	.container-fluid
		overflow hidden
		position relative
		figure
			margin 0
			padding 0
			img
				height auto
				margin-left -50%
				width 200%
		.inner-logo
			background-color amarillo
			border-bottom 6px solid rojo
			padding 10px 0
			text-align center
			&.galex-kids
				background-color #EA098D
			&.galex-bowl
				background-color rojo
	.all-content
		padding-bottom 20px
		padding-top 20px
		.find-branches
			width 100%
	.branches-container
		&.diversiones-galex
			.diversiones
				display none
		&.galex-kids
			.kids
				display none
		&.galex-bowl
			.bowl
				display none
		.col
			width 48%
			display inline-block
			position relative
			&:first-of-type
				margin-right 3%
			figure
				padding 0
				margin 0
				&.logo
					width 50%
					position absolute
					top 50%
					left 50%
					margin-left -25%
					margin-top -20%
				img
					width 100%
					height auto
	/*
	Join to the team
	*/
	.festeja-con-nosotros
		.title-with-shadow
			top initial
			bottom 15px
		.container-fluid figure img
			width 300%
			margin-left -75%
	.title-with-shadow
		color white
		font-size 3.6em
		font-style italic
		left 0
		letter-spacing 1px
		line-height .8em
		position absolute
		text-align center
		text-shadow 4px 0 0 azulMedio, -2px 0 0 azulMedio, 0 2px 0 azulMedio, 0 -2px 0 azulMedio, 2px 2px azulMedio, -2px -2px 0 azulMedio, 2px -2px 0 azulMedio, -2px 2px 0 azulMedio
		top 20px
		width 100%
	/*
	Party
	*/
	.party-title
		color rojo
		font-size 1.4em
		text-align center
		span
			background-color #F6F6F6
			display inline-block
			padding-left 10px
			padding-right 10px
	hr
		border-top 2px solid rojo
		margin 0
		margin-top -13px
	.packs-container
		padding-top 30px
		.pack
			margin-bottom 40px
			.place-logo
				text-align center
				margin-bottom 20px
			.place-title
				font-size 1.4em
			.price
				text-align right
				span
					text-shadow 4px 0 0 azulClaro, -2px 0 0 azulClaro, 0 2px 0 azulClaro, 0 -2px 0 azulClaro, 2px 2px azulClaro, -2px -2px 0 azulClaro, 2px -2px 0 azulClaro, -2px 2px 0 azulClaro
					font-size 3em
					color white
	/*
	Sucursales
	*/
	.sucursales
		.place
			background-color transparent
			margin 50px 0
			.title
				font-size 1.2em
				a
					color azulClaro
			figure
				border 6px solid rojo
				margin 0
				margin-top 5px
				padding 0
				img
					display block
					height auto
					width 100%
	.site-footer
		background-color azul
		overflow hidden
		padding 20px 5% 15px 5%
		.logo-footer
			display block
			margin 0 auto
			width 100px;
			img
				display block
				height auto
				width 100%
		.social-links
			margin 0
			padding 0
			padding-top 20px
			text-align center
			li
				background-color rojo
				display inline-block
				height 33px
				margin-left 10px
				width 32px
				a
					color white
					display block
					font-size 2em
					height 100%
					width 100%
@media screen and (min-width:380px)
	body
		.promos-banner
			.item
				.bottom
					margin-top -60px
@media screen and (min-width:480px)
	.btn.btn-primary
		font-size 1.4em
	body
		.main-banner
			.item
				img
					width 110%
					margin-left 0
		.brands
			text-align center
			.block-unity
				margin -50px 3% 0 0
				padding 0
				position relative
				width 30%
				display inline-block
				&:last-of-type
					margin-right 0
		.promos-banner
			.item
				.title
					text-align left
					padding 0 5%
					img
						width 65%
				.top
					display block
					min-height 100px
					overflow hidden
					ul
						li
							img
								margin-left -5%
								width 110%
				.bottom
					color white
					margin-top -100px
					padding-bottom 20px
					position relative
		.place
			.section-title
				text-align center
				img
					width 60%
			.place-carousel
				.item
					.title
						font-size 1.6em
						height 50px
						line-height 50px
						margin-top -50px
						padding 0 5%
						position relative
					.info
						font-size 1.2em
						.view-more
							font-size 1.1em
		.find-branches
			width 90%
			a
				font-size 1.4em
@media screen and (min-width:580px)
	body
		.promos-banner
			.item
				.bottom
					margin-top -30px
