		.styled-select select
			font-size 1.2em
			line-height 1.2em
		.styled-select
			margin 0 auto
			width 400px
	body
		.navbar-nav
			ul
				padding 18px 0
				text-align right
				li
					display inline-block
					margin-left 3px
					font-size 1.2em
					margin-right 10px
