						margin-right 0
					a
						color white
		.main-banner
			.item
				img
					width 100%
					margin-left 10%
		.place
			position relative
			border-top 6px solid rojo
			.section-title
				display none
			.place-carousel
				.item
					.title
						position absolute
						width 100%
						top 0
						z-index 10
						margin-top 0
					.inner-section-title
						display block
						padding-left 5%
						margin-top -100px
						height 100px
						img
							width 300px
					.map-info-container
						vertical-align middle
						.place-map
							width 41%
							display inline-block
							img
								width 90%
								height auto
						.info
							width 54%
