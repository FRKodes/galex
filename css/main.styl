						font-size 1.5em
					.price
						text-align center
						span
							font-size 4em
		/*
		Sucursales
		*/
		.sucursales
			.place
				background-color transparent
				border none
				display inline-block
				margin 20px 3% 20px 0
				width 30%
				.title
					font-size 1.4em
		/*Joint to the team @768px*/
